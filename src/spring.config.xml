<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="admin" class="com.next.pibelnext.Administrator">
		<property name="idAdministrator" value="7777"></property>
		<property name="name" value="super"></property>
		<property name="adress" ref="adress"></property>
	</bean>

	<bean name="adress" class="com.next.pibelnext.Adress">
		<constructor-arg name="road" value="road"></constructor-arg>
		<constructor-arg name="zip" value="1212"></constructor-arg>
	</bean>
</beans>
